{
  "name": "countdown-pro-for-gutenberg",
  "version": "1.0.0",
  "description": "A gutenberg block that allows adding countdown easily to your content",
  "main": "block/countdown.build.js",
  "scripts": {
    "dev": "cross-env webpack --watch",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "makepot": "wp i18n make-pot . languages/cpfg.pot",
    "makejson": "wp i18n make-json languages",
    "renamejson": "rename 's/(cpfg-[a-zA-Z_]+-)[^\\.]*(.json)/$1cpfg-block$2' languages/*"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@wordpress/babel-preset-default": "^6.10.0",
    "@wordpress/scripts": "^23.0.0",
    "autoprefixer": "^10.4.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "mini-css-extract-plugin": "^2.6.0",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.5.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "author": "Ali Khallad",
  "license": "GPLv3"
}
